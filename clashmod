function Exit()
print("Ø®Ø±ÙˆØ¬ ")
gg.skipRestoreState()
  os.exit()
  gg.setVisible(true)
end 

while true do
  if gg.isVisible(true) then
    ClashServer = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if ClashServer == 1 then
    HOME()
 end
